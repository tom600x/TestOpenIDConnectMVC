<div class="text-left">
    <table>
        <tr>
            <th style="width: 30%;">Claim Type</th>
            <th style="width: 30%;">Claim Value</th>
        </tr>
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
    <br />
    <h2>User.Identity.Name = @User.Identity.Name</h2>
</div>
